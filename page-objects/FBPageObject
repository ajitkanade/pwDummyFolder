class Facebook{
   constructor(page){
   this.loginID=page.locator("//input[@data-testid='royal-email']")
   this.password=page.locator("//input[@data-testid='royal-pass']")
   this.loginButton=page.locator("//button[@data-testid='royal-login-button']")
   }

   async login(username,password){
    await this.loginID.fill(username)
    await this.password.fill(password)
   await  this.loginButton.click()

   }
}

module.exports={Facebook}